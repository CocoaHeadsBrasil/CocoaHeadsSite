<%- model_class = Cidade -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.new', 'New %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>
<%= form_for(:cidade, :url => {:action => :create}, :html => { :class => 'form-horizontal' }) do |f| %>
<%= render :partial => 'form', :locals => {:f => f, :estados => @estados} %>
<% end %>
