<%- model_class = Video -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>
<%= form_for(@video, :url => {:action => :update, :palestrante_id => @palestrante.id}, :html => { :class => 'form-horizontal' }) do |f| %>
<%= render :partial => 'form', :locals => {:f => f, :palestrantes => @palestrantes, :agendas => @agendas} %>
<% end %>
